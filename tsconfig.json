{
  "compilerOptions": {
    "target": "es2017" /* Target latest ECMAScript features */,
    "module": "commonjs",
    "outDir": "./dist/",
    "strict": true /* Enable all strict type-checking options */,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
